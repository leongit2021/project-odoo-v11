<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="stock_picking_form_st" model="ir.ui.view">
            <field name="name">stock.picking.form.st</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/group[1]/group[2]/field[@name='origin']" position="after">
                    <field name="custom_clearance_id" options="{'no_create': True,'no_create_edit': True}"/>
                    <field name="bc_two_eight_id" options="{'no_create': True,'no_create_edit': True}"/>
                </xpath>
            </field>
        </record>

</odoo>
